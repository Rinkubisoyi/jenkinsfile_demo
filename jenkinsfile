    agent any

    stages {
        stage('build') {
            steps {
                echo 'building'
            }
        }
        stage('test') {
            steps {
                echo 'testing'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploying'
            }
        }
        stage('realease') {
            steps {
                echo 'releasing'
            }
        }
        stage('finish') {
            steps {
                echo 'finishing'
            }
        }
    }
}
